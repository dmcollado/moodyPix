    <%= render 'form', picture: @picture %>

    <% @pictures.each_with_index do |picture, index| %>
      <div id="pic-<%= picture.id %>">
        <a href="<%= picture_path(picture) %>" class="img-box" style="background-image:url('<%= picture.url %>');">
        </a>
          <%= picture.title %>
      </div>
    <% end %>
    </div>

<p id="notice"><%= notice %></p>